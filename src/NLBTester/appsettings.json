{
  "ConnectionStrings": {
    "SnCrMsSql": ""
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/nlbtester-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "sensenet": {
    "repository1": {
      "url": "https://localhost:44362",
      "authentication": {
        "ClientId": "",
        "ClientSecret": "",
        "ApiKey": ""
      }
    },
    "repository2": {
      "url": "https://localhost:44372",
      "authentication": {
        "ClientId": "",
        "ClientSecret": "",
        "ApiKey": ""
      }
    }
  },
  "NlbTest": {
    "WriteOperations": {
      "ThreadCount": 1,
      "StepDelay": 200
    },
    "ReadOperations": {
      "ThreadCount": 1,
      "StepDelay": 200
    },
    "Backup": {
      "InitialDelaySeconds": 11,
      "ThreadCount": 1,
      "StepDelay": 200,
      "IndexTarget": "",
      "DatabaseTarget": "",
      "BackupGapSeconds": 5
    }
  }
}